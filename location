    <h2>Location on Google Map</h2>
    <!-- Replace YOUR_LATITUDE and YOUR_LONGITUDE with the actual coordinates -->
    <iframe
        width="600"
        height="450"
        frameborder="0"
        style="border:0"
        src="https://www.google.com/maps/embed/v1/view?key=YOUR_API_KEY&center=YOUR_LATITUDE,YOUR_LONGITUDE&zoom=15"
        allowfullscreen
    ></iframe>
